&t_SI = "\<Esc>[6 q"
&t_SR = "\<Esc>[4 q"
&t_EI = "\<Esc>[2 q"
&t_Cs = "\e[4:3m"
&t_Ce = "\e[4:0m"

def OpenMyVimrcSmart()
  var vimrc = expand('$MYVIMRC')

  var wid = bufwinid(bufnr(vimrc))
  if wid != -1
    # 1) already open? jump to its window
    win_gotoid(wid)
  elseif &modified
    # 2) not open, but current buffer modified? open in new tab
    execute('tabe ' .. fnameescape(vimrc))
  else
    # 3) not open, current buffer unmodified? reuse current window
    execute('edit ' .. fnameescape(vimrc))
  endif
enddef

noremap <silent> <C-,> <ScriptCmd>OpenMyVimrcSmart()<CR>

